<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Qty_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISBLANK(  Quantity__c      ) , NOT( ISBLANK( Date_Booked__c ) ))</errorConditionFormula>
    <errorDisplayField>Quantity__c</errorDisplayField>
    <errorMessage>Please enter a Quantity</errorMessage>
</ValidationRule>
