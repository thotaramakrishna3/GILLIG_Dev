<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Start_Date_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISBLANK( Start_Date__c ) , NOT( ISBLANK( Date_Booked__c ) ))</errorConditionFormula>
    <errorDisplayField>Start_Date__c</errorDisplayField>
    <errorMessage>Please enter a Start Date</errorMessage>
</ValidationRule>
