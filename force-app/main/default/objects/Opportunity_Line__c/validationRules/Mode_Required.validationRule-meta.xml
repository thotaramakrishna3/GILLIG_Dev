<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Mode_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISBLANK(  Mode__c  ) , NOT( ISBLANK( Date_Booked__c ) ))</errorConditionFormula>
    <errorDisplayField>Mode__c</errorDisplayField>
    <errorMessage>Please enter a Mode</errorMessage>
</ValidationRule>
