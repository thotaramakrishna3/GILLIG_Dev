<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PO_Amount_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISBLANK(  PO_Amount__c   ) , NOT( ISBLANK( Date_Booked__c ) ))</errorConditionFormula>
    <errorDisplayField>PO_Amount__c</errorDisplayField>
    <errorMessage>Please enter a PO Amount</errorMessage>
</ValidationRule>
