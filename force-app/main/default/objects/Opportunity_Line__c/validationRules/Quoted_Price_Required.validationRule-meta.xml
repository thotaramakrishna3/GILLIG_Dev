<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Quoted_Price_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISBLANK( Quoted_Price__c ) , NOT( ISBLANK( Date_Booked__c ) ))</errorConditionFormula>
    <errorDisplayField>Quoted_Price__c</errorDisplayField>
    <errorMessage>Please enter a Quoted Price</errorMessage>
</ValidationRule>
