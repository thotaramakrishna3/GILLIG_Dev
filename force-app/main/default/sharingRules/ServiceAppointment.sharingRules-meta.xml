<?xml version="1.0" encoding="UTF-8"?>
<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingOwnerRules>
        <fullName>Field_Service</fullName>
        <accessLevel>Read</accessLevel>
        <description>Read only sharing rule for FS managers and reps</description>
        <label>Field Service Read Only</label>
        <sharedTo>
            <roleAndSubordinatesInternal>Field_Service_Mgr</roleAndSubordinatesInternal>
        </sharedTo>
        <sharedFrom>
            <allInternalUsers></allInternalUsers>
        </sharedFrom>
    </sharingOwnerRules>
    <sharingOwnerRules>
        <fullName>Field_Service_Coordinator</fullName>
        <accessLevel>Edit</accessLevel>
        <label>Field Service Coordinator</label>
        <sharedTo>
            <role>Field_Service_Coordinator</role>
        </sharedTo>
        <sharedFrom>
            <allInternalUsers></allInternalUsers>
        </sharedFrom>
    </sharingOwnerRules>
    <sharingOwnerRules>
        <fullName>Service_Appointment_Read</fullName>
        <accessLevel>Read</accessLevel>
        <label>Service Appointment - Read</label>
        <sharedTo>
            <roleAndSubordinatesInternal>Field_Service_Mgr</roleAndSubordinatesInternal>
        </sharedTo>
        <sharedFrom>
            <role>Field_Service_Coordinator</role>
        </sharedFrom>
    </sharingOwnerRules>
    <sharingOwnerRules>
        <fullName>Write_Access_to_SA</fullName>
        <accessLevel>Edit</accessLevel>
        <description>Read/Write access to Service Appointment - Salesforce Admin</description>
        <label>Write Access to SA</label>
        <sharedTo>
            <role>IT</role>
        </sharedTo>
        <sharedFrom>
            <allInternalUsers></allInternalUsers>
        </sharedFrom>
    </sharingOwnerRules>
</SharingRules>
